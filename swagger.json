  "components": {
    "schemas": {
      "Login": {
        "type": "object",
        "required": ["email", "password"],
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      },
      "Users": {
        "type": "object",
        "required": ["firstName", "lastName", "email", "password"],
        "properties": {
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string",
            "minLength": 8
          }
        }
      },
      "Actors": {
        "type": "object",
        "required": ["actor_id", "name"],
        "properties": {
          "actor_id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "pattern": "/([0-9]{4})-(?:[0-9]{2})-([0-9]{2})/",
            "example": "2021-03-05"
          },
          "updatedAt": {
            "type": "string",
            "pattern": "/([0-9]{4})-(?:[0-9]{2})-([0-9]{2})/",
            "example": "2021-03-05"
          }
        }
      },
      "GetActor": {
        "type": "object",
        "required": ["id"],
        "properties": {
          "id": {
            "type": "integer"
          }
        }
      },
      "UpdateActor": {
        "type": "object",
        "required": ["id"],
        "properties": {
          "id": {
            "type": "integer"
          }
        }
      },
      "DeleteActor": {
        "type": "object",
        "required": [""],
        "properties": {}
      },
      "Directors": {
        "type": "object",
        "required": ["id", "name"],
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "pattern": "/([0-9]{4})-(?:[0-9]{2})-([0-9]{2})/",
            "example": "2021-03-05"
          },
          "updatedAt": {
            "type": "string",
